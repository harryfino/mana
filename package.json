{
	"name": "mana-font",
	"version": "1.17.0",
	"description": "Magic: the Gathering mana symbol pictographic font",
	"main": "css/mana.css",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andrewgioia/mana.git"
	},
	"keywords": [
		"mana",
		"mtg",
		"mana",
		"symbols",
		"magic",
		"gathering",
		"font"
	],
	"scripts": {
		"sass": "sass sass/mana.scss dist/mana.css --style=expanded",
		"sassmin": "sass sass/mana.scss dist/mana.min.css --style compressed",
		"build": "rsync -avzh ./fonts/mana* ./dist/ && yarn sassmin"
	},
	"author": "Andrew Gioia <andrew@gioia.email>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andrewgioia/mana/issues"
	},
	"homepage": "https://mana.andrewgioia.com",
	"devDependencies": {
		"sass": "^1.80.4"
	}
}